max(50, \"CNIC no is invalid\")\r\n      .required(\"Address is required\"),\r\n  });\r\n  return (\r\n    <>\r\n      <Formik\r\n        initialValues={{\r\n          fullName: \"\",\r\n          email: \"\",\r\n          mobileNo: \"\",\r\n          cnicNo: \"\",\r\n          city: \"\",\r\n          status: \"\",\r\n          address: \"\",\r\n        }}\r\n        validationSchema={validate}\r\n        onSubmit={(values) => {\r\n          // console.log(values);\r\n        }}\r\n      >\r\n        {(formik) => (\r\n          <div>\r\n            <h2 className='my-4 font-weight-bold-display-4'>\r\n              Add New Employee\r\n            </h2>\r\n            <Form onSubmit={(e) => onSubmit(e)}>\r\n              <div className='row'>\r\n                <div className='col-6'>\r\n                  <FormFields\r\n                    label='Full Name'\r\n                    placeholder='Wahab Khan '\r\n                    name='fullName'\r\n                    type='text'\r\n                    value={fullName}\r\n                    onChange={(e) => onInputChange(e)}\r\n                  />\r\n                </div>\r\n                <div className='col-6'>\r\n                  <FormFields\r\n                    label='Email'\r\n                    placeholder='wahab@gmail.com'\r\n                    name='email'\r\n                    type='email'\r\n                    value={email}\r\n                    onChange={(e) => onInputChange(e)}\r\n                  />\r\n                </div>\r\n              </div>\r\n              <div className='row'>\r\n                <div className='col-6'>\r\n                  <FormFields\r\n                    label='Mobile no'\r\n                    placeholder='123123'\r\n                    name='mobileNo'\r\n                    type='tel'\r\n                    value={mobileNo}\r\n                    onChange={(e) => onInputChange(e)}\r\n                  />\r\n                </div>\r\n                <div className='col-6'>\r\n                  <FormFields\r\n                    label='CNIC no'\r\n                    placeholder='234545'\r\n                    name='cnicNo'\r\n                    type='tel'\r\n                    value={cnicNo}\r\n                    onChange={(e) => onInputChange(e)}\r\n                  />\r\n                </div>\r\n              </div>\r\n              <div className='row'>\r\n                <div className='col-6'>\r\n                  <FormFields\r\n                    label='City'\r\n                    placeholder='Hyderabad'\r\n                    name='city'\r\n                    type='text'\r\n                    value={city}\r\n                    onChange={(e) => onInputChange(e)}\r\n                  />\r\n                </div>\r\n                <div className='col-6'>\r\n                  <FormFields\r\n                    label='Status'\r\n                    placeholder='Hire/Not Hire'\r\n                    name='status'\r\n                    type='text'\r\n                    value={status}\r\n                    onChange={(e) => onInputChange(e)}\r\n                  />\r\n                </div>\r\n              </div>\r\n              <div className='row'>\r\n                <div className='col-md-12'>\r\n                  <FormFields\r\n                    label='Address'\r\n                    placeholder='house no 234'\r\n                    name='address'\r\n                    type='text'\r\n                    value={address}\r\n                    onChange={(e) => onInputChange(e)}\r\n                  />\r\n                  <button\r\n                    type='submit'\r\n                    className='btn btn-primary'\r\n                    style={{\r\n                      padding: \"10px 20px\",\r\n                      fontSize: \"15px\",\r\n                      marginTop: \"20px\",\r\n                    }}\r\n                  >\r\n                    Submit\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </Form>\r\n          </div>\r\n        )}\r\n      </Formik>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default index;\r\n"]},"metadata":{},"sourceType":"module"}