 {\n    if (fileUpload == null) return;\n    const fileRef = ref(storage, `file/${fileUpload.name}`);\n\n    const desertRef = ref(storage, `file/${fileUpload.id}`);\n    const deleteFile = async (id) => {\n      deleteObject(desertRef)\n        .then(() => {\n          console.log(\"delete\");\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    };\n\n    uploadBytes(fileRef, fileUpload).then((snapshot) => {\n      // console.log(snapshot);\n      getDownloadURL(snapshot.ref).then((url) => {\n        setFileList((prev) => [...prev, url]);\n      });\n      alert(\"File Uploaded\");\n    });\n  };\n\n  useEffect(() => {\n    listAll(fileListRef).then((res) => {\n      // console.log(res);\n      res.items.forEach((item) => {\n        // console.log(item.name);\n        getDownloadURL(item).then((url) => {\n          setFileList((prev) => [...prev, { url, name: item.name }]);\n        });\n      });\n    });\n  }, []);\n  return (\n    <>\n      <ContainerDefault title='File Manage'>\n        <HeaderDashboard title='File Management' description='File Listing ' />\n\n        <div className='p-4'>\n          <input\n            type='file'\n            onChange={(e) => {\n              setFileUpload(e.target.files[0]);\n            }}\n          />\n          <button onClick={uploadFile}>Upload File</button>\n        </div>\n        <div>\n          <div className='col-6'>\n            <input\n              className={style.input}\n              type='text'\n              placeholder='Search Employee...'\n              onChange={(event) => {\n                setSearchTerm(event.target.value);\n              }}\n            />\n          </div>\n          {/* {fileList.map((item) => {\n            return (\n              <a href={item.url} target='_blank'>\n                {\" \"}\n                {item.name}{\" \"}\n              </a>\n            );\n          })} */}\n\n          <table className='table border shadow mt-4'>\n     